{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "tyranoscript",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_line"
          },
          {
            "type": "SYMBOL",
            "name": "blank_line"
          }
        ]
      }
    },
    "_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment"
            },
            {
              "type": "SYMBOL",
              "name": "label"
            },
            {
              "type": "SYMBOL",
              "name": "tag_line"
            },
            {
              "type": "SYMBOL",
              "name": "text_line"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\r?\\n"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "tag_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tag"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "tag"
              },
              {
                "type": "SYMBOL",
                "name": "inline_text"
              }
            ]
          }
        }
      ]
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "tag_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "tag_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "attribute": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "name",
              "content": {
                "type": "SYMBOL",
                "name": "attribute_name"
              }
            },
            {
              "type": "STRING",
              "value": "="
            },
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "attribute_value"
              }
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "attribute_name"
          }
        }
      ]
    },
    "attribute_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "attribute_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "unquoted_value"
        }
      ]
    },
    "quoted_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "string_content"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "string_content": {
      "type": "PATTERN",
      "value": "[^\"]*"
    },
    "unquoted_value": {
      "type": "PATTERN",
      "value": "[^\\s\\]\"]+"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "PATTERN",
          "value": "[^\\r\\n]*"
        }
      ]
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "SYMBOL",
          "name": "label_name"
        }
      ]
    },
    "label_name": {
      "type": "PATTERN",
      "value": "[^\\s\\r\\n]+"
    },
    "text_line": {
      "type": "PATTERN",
      "value": "[^\\[;*\\r\\n \\t][^\\[\\r\\n]*"
    },
    "inline_text": {
      "type": "PATTERN",
      "value": "[^\\[\\r\\n]+"
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "[ \\t]*\\r?\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    }
  ],
  "conflicts": [
    [
      "tag_line"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
